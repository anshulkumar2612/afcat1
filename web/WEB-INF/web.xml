<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>javafiles.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogout</servlet-name>
        <servlet-class>javafiles.UserLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserSignup</servlet-name>
        <servlet-class>javafiles.UserSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserResetPassword</servlet-name>
        <servlet-class>javafiles.UserResetPassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterAfcat</servlet-name>
        <servlet-class>javafiles.RegisterAfcat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/userLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogout</servlet-name>
        <url-pattern>/userLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserSignup</servlet-name>
        <url-pattern>/userSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserResetPassword</servlet-name>
        <url-pattern>/userSetPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterAfcat</servlet-name>
        <url-pattern>/registerAfcat</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
